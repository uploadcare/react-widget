{
  "name": "@uploadcare/react-widget",
  "version": "1.2.0",
  "description": "React component for Uploadcare Widget",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "types/index.d.ts",
  "scripts": {
    "lint:types": "dtslint types --onlyTestTsNext",
    "lint:js": "eslint ./src/** ./test/** karma.config.js rollup.config.js",
    "clean": "rimraf dist en",
    "build": "rollup -c",
    "dummy": "parcel dummy/index.html",
    "prepublishOnly": "npm run clean && npm run build",
    "test:dev": "karma start karma.config.js",
    "format": "prettier --write \"./src/**/*.js\" \"./test/**/*.js\" karma.config.js rollup.config.js",
    "test": "npm run lint:js && npm run lint:types && karma start karma.config.js --single-run --nocache",
    "release:prepare": "shipjs prepare",
    "release:trigger": "shipjs trigger"
  },
  "files": [
    "dist/*",
    "en/*",
    "locales.js",
    "types/index.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uploadcare/react-widget.git"
  },
  "keywords": [
    "react",
    "component",
    "uploadcare",
    "widget",
    "uploader",
    "filepicker",
    "dialog",
    "upload",
    "store",
    "file",
    "files",
    "image",
    "images"
  ],
  "authors": [
    "Ivakhnenko Dmitry"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/uploadcare/react-widget/issues"
  },
  "homepage": "https://github.com/uploadcare/react-widget",
  "dependencies": {
    "@uploadcare/client-suspense": "^1.0.2",
    "react-fast-compare": "^2.0.4",
    "uploadcare-widget": "^3.8.0"
  },
  "peerDependencies": {
    "react": "^16.10.2",
    "react-dom": "^16.10.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "@testing-library/react": "^9.3.0",
    "@types/jquery": "^3.3.31",
    "@types/react": "^16.9.11",
    "assert": "^2.0.0",
    "babel-eslint": "^10.0.3",
    "dtslint": "^2.0.0",
    "eslint": "^6.4.0",
    "eslint-config-prettier": "^6.4.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-jsx": "^8.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^2.0.1",
    "eslint-plugin-standard": "^4.0.1",
    "karma": "^4.3.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-mocha": "^1.3.0",
    "karma-rollup-preprocessor": "^7.0.2",
    "mocha": "^6.2.1",
    "parcel-bundler": "^1.12.4",
    "prettier": "^1.19.1",
    "puppeteer": "^1.20.0",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "rimraf": "^3.0.0",
    "rollup": "^1.23.1",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "shipjs": "0.11.3",
    "typescript": "^3.7.2"
  }
}
